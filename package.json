{
  "name": "pos",
  "version": "1.0.1",
  "main": "main.js",
  "scripts": {
    "start": "electron . && nodemon backend/server.js",
    "build": "electron-packager . POS-ov --icon=pos.ico",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder"
  },
  "repository": {},
  "build": {
    "appId": "pos.dftech.in",
    "productName": "POS Infinity",
    "win": {
      "icon": "pos.ico",
      "target": "nsis",
